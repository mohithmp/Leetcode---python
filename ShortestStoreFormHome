class Solution():
    def sol(self,M,N):
        D=[]
        for i in range(len(N)):
            a=abs(N[i]-M[0])
            n=M[0]
            for j in range(1,len(M)):
                if abs(M[j]-N[i])<a:
                    a=abs(M[j]-N[i])
                    n=M[j]
                if abs(M[j]-N[i])==a and n>M[j]:
                    n=M[j]
                    
            D.append(n)
        return D
        
M=[5,6,8,1,4,2]
N=[1,8,5,7,3,11,7,3]
print(Solution().sol(M,N))
